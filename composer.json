{
    "name": "keen-io/keen-io-zf2",
    "type": "library",
    "license": "MIT",
    "description" : "Zend Framework 2 module for using KeenIO",
    "keywords": [
        "keen",
        "keen.io",
        "events",
        "analytics",
        "zf2"
    ],
    "homepage": "https://github.com/keenlabs/KeenClient-PHP-ZF2",
    "authors": [
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "keen-io/keen-io": "~2.0",
        "zendframework/zend-modulemanager": "~2.0",
        "zendframework/zend-servicemanager": "~2.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require-dev": {
        "zendframework/zendframework": "2.*",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "KeenIOModule\\": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "scripts": {
      "phpcs": [
        "./vendor/bin/phpcs --colors --standard=PSR2 src"
      ],
      "phpunit": [
        "./vendor/bin/phpunit"
      ]
    }
}
